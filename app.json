{
  "expo": {
    "name": "Pongo",
    "slug": "pongo",
    "version": "3.9.0",
    "scheme": "pongo",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#AEB0AF"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.engd.villapongo",
      "buildNumber": "3.9.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets.",
        "NSLocationWhenInUseUsageDescription": "This app uses location services to find events near you.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses location services to find events near you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#AEB0AF"
      },
      "package": "com.engd.villapongo",
      "versionCode": 9,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "production",
          "smallIcons": [
            "./assets/small_icon.png"
          ],
          "largeIcons": [
            "./assets/large_icon.png"
          ],
          "smallIconAccentColor": "#AEB0AF"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permita que a Pongo acesse sua c√¢mera.",
          "microphonePermission": "Permita que a Pongo acesse seu microfone.",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "oneSignalAppId": "a972b3c3-b43c-4468-9aef-298babfece52",
      "eas": {
        "projectId": "c9747e9a-3ac6-4a79-a1fd-b3c992d47f11"
      }
    }
  }
}